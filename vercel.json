{
  "version": 2,
  "builds": [
    {
      "src": "emergency_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "emergency_vercel.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "PYTHONPATH": "."
  },
  "functions": {
    "emergency_vercel.py": {
      "maxDuration": 30
    }
  }
}